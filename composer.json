{
    "name": "midnite81/geolocation",
    "description": "A laravel package to hook into IP Info DB Service",
    "keywords": [
        "sms",
        "laravel"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Simon Rogers",
            "email": "webdev@centralblue.co.uk"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "kylekatarnls/update-helper": "^1.1",
        "laravel/framework": "^5.0|^6.0|^7.0",
        "guzzlehttp/guzzle": "^6.2",
        "mockery/mockery": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5",
        "sempro/phpunit-pretty-print": "^1.0",
        "satooshi/php-coveralls": "1.0.*"
    },
    "autoload": {
        "psr-4": {
            "Midnite81\\GeoLocation\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Midnite81\\GeoLocation\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "UpdateHelper\\UpdateHelper::check"
        ]
    },
    "extra": {
        "component": "package",
        "frameworks": [
            "Laravel 5",
            "Laravel 6",
            "Laravel 7"
        ],
        "update-helper": "Midnite81\\GeoLocation\\Upgrade"
    },
    "minimum-stability": "stable"
}
